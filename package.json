{
  "name": "search-apps",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "usecase:install": "cd usecase && npm install",
    "usecase:create": "schematics ./usecase:usecase --debug=false --force",
    "usecase:build": "cd usecase && npm run build",
    "prepare": "husky install",
    "format": "prettier --write src/**/*.ts{,x}"
  },
  "dependencies": {
    "@angular-devkit/schematics-cli": "^14.2.2",
    "@emotion/react": "^11.10.0",
    "@emotion/styled": "^11.10.0",
    "@mui/icons-material": "^5.8.4",
    "@mui/material": "^5.10.1",
    "@mui/x-date-pickers": "^5.0.0",
    "copy-image-clipboard": "^2.1.2",
    "dayjs": "^1.11.5",
    "dayjs-plugin-utc": "^0.1.2",
    "file-saver": "^2.0.5",
    "jspdf": "^2.5.1",
    "mui-file-dropzone": "^4.0.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "timezone-abbreviations": "^1.2.6"
  },
  "devDependencies": {
    "@types/file-saver": "^2.0.5",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "@vitejs/plugin-react": "^2.1.0",
    "husky": "^8.0.1",
    "prettier": "^2.7.1",
    "typescript": "*",
    "vite": "^3.1.0"
  }
}
